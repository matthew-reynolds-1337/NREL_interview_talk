\begin_inset Formula 
\begin{eqnarray*}
-\nabla\cdot(a(\mathbf{x},\mathbf{z})\nabla u(\mathbf{x},\mathbf{z})) & = & 1,\quad\mathbf{x}\in\mathcal{D},\\
u(\mathbf{x},\mathbf{z}) & = & 0,\quad\mathbf{x}\in\partial\mathcal{D},
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Standard
defined on 
\begin_inset Formula $\mathcal{D}=(0,1)\times(0,1)$
\end_inset

 with boundary 
\begin_inset Formula $\partial\mathcal{D}$
\end_inset

.
 
\end_layout

\begin_layout Itemize
\begin_inset Formula $a(\mathbf{x},\mathbf{z})=a_{0}+\sigma_{a}\sum_{k=1}^{d}\sqrt{\zeta_{k}}\varphi_{k}(\mathbf{x})z_{k}$
